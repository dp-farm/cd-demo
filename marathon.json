{
    "container": {
        "docker": {
            "image": "mesosphere/cd-demo-app:latest",
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 4000,
                    "protocol": "tcp"
                }
            ]
        },
        "type": "DOCKER"
    },
    "cpus": 0.1,
    "healthChecks": [
        {
            "gracePeriodSeconds": 600,
            "intervalSeconds": 30,
            "maxConsecutiveFailures": 2,
            "portIndex": 0,
            "protocol": "TCP",
            "timeoutSeconds": 10
        }
    ],
    "id": "cd-demo-app",
    "instances": 5,
    "labels": {
    },
    "mem": 128,
    "upgradeStrategy": {
        "minimumHealthCapacity": 0
    },
    "env": {
	    "NUAGE-ENTERPRISE": "Citrix",
	    "NUAGE-DOMAIN" : "myDomainInstance",
	    "NUAGE-ZONE":"MyZone",
	    "NUAGE-NETWORK":"web-frontend",
	    "NUAGE-USER":"admin"
    }
}
