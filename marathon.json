{
    "container": {
        "docker": {
            "image": "mesosphere/cd-demo-app:latest",
            "network": "NONE",
        },
        "type": "DOCKER"
    },
    "cpus": 0.1,
    "ports": [
	    4000
     ],
    "id": "cd-demo-app",
    "instances": 5,
    "labels": {
	"hello" : "how are you"
    },
    "mem": 128,
    "upgradeStrategy": {
        "minimumHealthCapacity": 0
    },
    "env": {
	    "NUAGE-ENTERPRISE": "Citrix",
	    "NUAGE-DOMAIN" : "myDomainInstance",
	    "NUAGE-ZONE":"MyZone",
	    "NUAGE-NETWORK":"web-frontend",
	    "NUAGE-USER":"admin"
    }
}
